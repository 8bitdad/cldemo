<VirtualHost *:80>
	DocumentRoot /var/www
	ServerName <%= scope.lookupvar('::hostname') %>
	Alias /sflow-rt /var/www/sflow-rt
	<Directory "/var/www/sflow-rt">
	Options FollowSymLinks
	AllowOverride All
	Order allow,deny
	Allow from all
	Deny from none
	</Directory>
        ErrorLog /var/log/apache2/error.log
        LogLevel warn
</VirtualHost>
